{
  "name": "yourname/rest-api-skeleton",
  "description": "Clean PHP REST API skeleton with Docker and Swagger UI",
  "type": "project",
  "require": {
    "php": "^8.4",
    "vlucas/phpdotenv": "^5.6",
    "doctrine/orm": "^3.3",
    "ramsey/uuid": "^4.7",
    "symfony/cache": "^7.3",
    "doctrine/migrations": "^3.9"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.7",
    "doctrine/doctrine-fixtures-bundle": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public"
  }
}
